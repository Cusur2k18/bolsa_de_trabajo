<%= simple_form_for @phone_number do |phone| %>
	<%= phone.input :description, label: t('phone_numbers.description') %>
	<%= phone.input :phone_number, label: t('phone_numbers.phone_number')  %>
	<%= phone.button :submit %>
	<%= link_to t('educations.index.go_back'), student_dashboard_path, class: 'btn btn-info' %>
<% end %>
